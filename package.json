{
  "name": "ng-timesheet",
  "version": "0.1.7",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "npm run config:dev && ng serve",
    "debug": "npm run config:dev && ng serve -c=debug",
    "build": "npm run config:prod && ng build",
    "build:ci": "npm run config:prod && ng build -c=ci --no-progress",
    "local": "node ./scripts/live-server-config.js",
    "lighthouse":
      "node ./scripts/lighthouse.js > lighthouse-report.html && npm run view:lighthouse",
    "view:lighthouse": "node ./scripts/view-report.js",
    "test": "ng test --source-map=false",
    "test:ci": "ng test -c=ci",
    "lint": "ng lint",
    "lint:p": "npx prettier --write 'src/**/*.ts'",
    "e2e": "ng e2e",
    "deploy": "npm run build && firebase deploy",
    "config": "ts-node ./scripts/set-env.ts",
    "config:dev": "npm run config -- --environment=dev --hmr=true",
    "config:prod": "npm run config -- --environment=prod",
    "precommit": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.0",
    "@angular/cdk": "^6.0.1",
    "@angular/common": "6.0.0",
    "@angular/compiler": "6.0.0",
    "@angular/core": "6.0.0",
    "@angular/flex-layout": "^6.0.0-beta.15",
    "@angular/forms": "6.0.0",
    "@angular/http": "6.0.0",
    "@angular/material": "^6.0.1",
    "@angular/platform-browser": "6.0.0",
    "@angular/platform-browser-dynamic": "6.0.0",
    "@angular/router": "6.0.0",
    "@angular/service-worker": "6.0.0",
    "angularfire2": "^5.0.0-rc.6",
    "angulartics2": "^6.0.0",
    "core-js": "^2.4.1",
    "firebase": "^4.10.1",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular/cli": "^6.0.0",
    "@angular/compiler-cli": "6.0.0",
    "@angular/language-service": "6.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "chrome-launcher": "^0.10.2",
    "codelyzer": "^4.3.0",
    "dotenv": "^5.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "^2.99.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lighthouse": "^2.9.4",
    "live-server": "^1.2.0",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.7.2"
  }
}
